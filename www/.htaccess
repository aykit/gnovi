Options FollowSymLinks

RewriteEngine on
RewriteBase /

RewriteRule ^graph$ graph/ [R,L]

RewriteRule ^$ pages/start.php
RewriteRule ^einloggen$ pages/login.php
RewriteRule ^ausloggen$ pages/logout.php
RewriteRule ^registrieren$ pages/register.php
RewriteRule ^profil$ pages/profile.php
RewriteRule ^anlauf$ pages/input.php

# This methods forbids passing a slash in encoded form (%2f) as a parameter
# since it is forbidden in this part of the url
RewriteRule ^graph/(.*)$ pages/graph.php?$1

RewriteRule ^([^/]+)$ pages/$1.php
